    {
        "id": "b55a96d50db6c208",
        "type": "function",
        "z": "6021a33e595f7f8d",
        "name": "function 4",
        "func": "let p = msg.payload;\n\n// rename time â†’ device_time\nif (p.time) {\n    p.device_time = p.time;\n    delete p.time;\n}\n\nmsg.measurement = \"weather_station\";\n\nmsg.fields = {\n    temperature: Number(p.temperature),\n    humidity: Number(p.humidity),\n    wind_speed: Number(p.wind_speed),\n    rain_mm: Number(p.rain_mm),\n    rain_analog: Number(p.rain_analog)\n};\n\nmsg.tags = {\n    wind_dir: String(p.wind_dir),\n    device_time: String(p.device_time)\n\n};\n\n// biarkan influx pakai timestamp server\nreturn msg;\n",
